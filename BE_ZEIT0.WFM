  CASE Smonat=11 
   lnPrÅfzeitMonat = Smin_nov
  CASE Smonat=12
   lnPrÅfzeitMonat = Smin_dez
 ENDCASE
 
 lcDatum = dmy(ctod("01."+ltrim(str(Smonat))+"."+Sjahr))
 lcDatum = left(right(lcDatum,len(lcDatum)-3)+space(15),15)
 
 lnPrÅfzeitMonat = round(lnPrÅfzeitMonat/60,0)
 Sprozent        = 100*lnPrÅfzeitMonat/Sges_prÅfzeit
 Sbalken         = REPL(CHR(176),ROUND(35*lnPrÅfzeitMonat/Sges_prÅfzeit,0))
 
 if Sprozent = 0
  @ Szeile,13 SAY lcDatum + "   0 h"
 else
  @ Szeile,13 SAY lcDatum + str(lnPrÅfzeitMonat,4) + " h  " + Sbalken + "  " + LTRIM(STR(ROUND(Sprozent,0),3)) + "%"
 endif 
 
 do case
  case Smonat = month(Stermin_bis)
