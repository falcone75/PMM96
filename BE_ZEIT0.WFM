   lnAnzPM_ZeitGleich0   = Spm2_juni
   lnPrÅfzeit            = Smin_juni
  CASE Smonat=7
   lnAnzPM_ZeitGroeszer0 = Spm1_juli
   lnAnzPM_ZeitGleich0   = Spm2_juli
   lnPrÅfzeit            = Smin_juli
  CASE Smonat=8
